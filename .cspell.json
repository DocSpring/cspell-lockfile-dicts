{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "ignorePaths": ["node_modules", "dist", "coverage", "test/test.txt"],
  "dictionaries": [
    "typescript",
    "node",
    "npm",
    "project-words",
    "test-words",
    "lockfile-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./.cspell/project-words.txt",
      "addWords": true
    },
    {
      "name": "test-words",
      "path": "./.cspell/test-words.txt",
      "addWords": true
    },
    {
      "name": "lockfile-words",
      "path": "./.cspell/lockfile-words.txt",
      "description": "Dictionary of words extracted from lockfiles"
    }
  ]
}
